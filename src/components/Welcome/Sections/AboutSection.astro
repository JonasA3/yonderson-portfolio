---
import { cvContent } from '../../../Content/texts';
import CvSection from '../SectionWrapper.astro';

const { about } = cvContent;
---

<CvSection id="about" title="About Jonas Andersson (me)">
  <ul class="font-body text-lg text-justify">
    <p>{about}</p>
    <img
      src="https://avatars.githubusercontent.com/u/1025460?v=4"
      alt="Jonas Andersson"
      class="rounded-full w-32 h-32 mb-4"
    />
  </ul>
</CvSection>
